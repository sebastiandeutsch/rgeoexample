<p id="notice"><%= notice %></p>

<h1>Listing Car Parks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>Zip</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_parks.each do |car_park| %>
      <tr>
        <td><%= car_park.name %></td>
        <td><%= car_park.street %></td>
        <td><%= car_park.zip %></td>
        <td><%= car_park.city %></td>
        <td><%= link_to 'Show', car_park %></td>
        <td><%= link_to 'Edit', edit_car_park_path(car_park) %></td>
        <td><%= link_to 'Destroy', car_park, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>

<div id="markerhash" class="hide">
  <%= @hash.to_json %>
</div>

<br>

<%= link_to 'New Car park', new_car_park_path %>
